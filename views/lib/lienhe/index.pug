    .truongthinh-form-v1
        .wrap-form
            div
                .row
                    h2.col-12 Quý khách vui lòng điền thông tin vào mẫu bên dưới và gửi những góp ý, thắc mắc cho Trường Thịnh Corp chúng tôi sẽ phản hồi email của Quý Khách trong thời gian sớm nhất.
                    .form-group.qtext.col-lg-6
                        label Họ tên
                        input(type="text" required)
                    .form-group.qtext.col-lg-6
                        label Email
                        input(type="text" required)
                    .form-group.qtext.col-lg-6
                        label Số điện thoại
                        input(type="text" required)
                    .form-group.col-lg-6
                        label Chọn phòng ban/ bộ phận liên hệ
                        select
                            option Chọn phòng ban/ bộ phận liên hệ
                            option Chọn phòng ban/ bộ phận liên hệ
                            option Chọn phòng ban/ bộ phận liên hệ
                    .form-group.qtext.col-12
                        label Tiêu đề
                        input(type="text" required)
                    .form-group.qparagraph.col-12
                        label Nội dung
                        textarea(name="", cols="30", rows="10")

